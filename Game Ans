#include<stdio.h>
#include<conio.h>
void main()
{
int i,m,j,w=0,n,flag;
clrscr();
printf("\nenter the value of m\n");

for(i=1;i<=m;i++)
{
scanf("%d",&i);
}
printf("\nenter the value of n");
scanf("%d",&n);
if(n>=m)
{
printf("\n n should be less than m");
}
flag=m;
 i=1;
 j=1;
 w=0;
while(flag!=1)
{
if(j>m)
{


j=1;

}

if(i==n)
{
    
i=1;
flag--;
m--;
j++;
     


    }
else
{

w=j;
i++;
j++;
}

}


 printf("\n%d is win",w);

getch();
}
